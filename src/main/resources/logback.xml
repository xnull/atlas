<configuration scan="true" scanPeriod="30 seconds">

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date{yy/MM/dd HH:mm:ss.SSS} %-5level %-10.10thread | %30.30logger{40} | %m%n</pattern>
        </encoder>
    </appender>

    <logger name="bynull.poi" level="trace"/>

    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>